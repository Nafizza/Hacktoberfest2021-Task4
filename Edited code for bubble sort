#include <stdio.h>
void swap(int *xp, int *yp)
{
    int temp = *xp;
    *xp = *yp;
    *yp = temp;
}
int bubble(int arr[],int n)
{
    int i,j;
    for (i = 0; i < n-1; i++)     
       for (j = 0; j < n-i-1; j++)
           if (arr[j] > arr[j+1])
              swap(&arr[j], &arr[j+1]);
}


int main()
{
    
    int n=5,j,i;
    int arr[5]={58,89,71,35,6};
    bubble(arr,n);
    
    for(i=0;i<n;i++)
        {
            printf("%d ",arr[i]);
        }
}
